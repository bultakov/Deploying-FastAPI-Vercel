{
  "version": 2,
  "builds": [
    {
      "src": "/api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    },
    { 
      "src": "/docs", 
      "dest": "api/main.py" 
    },
    { 
      "src": "/openapi.json", 
      "dest": "api/main.py"
    }
  ]
}